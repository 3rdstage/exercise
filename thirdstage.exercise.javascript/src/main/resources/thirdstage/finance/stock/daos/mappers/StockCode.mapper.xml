<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="thirdstage.finance.stock.StockCode">
	<select id="selectCountByCode" parameterType="java.lang.String">
select count(*) from stock_code where code like '%code%'
	</select>
</mapper>

