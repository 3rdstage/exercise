<?xml version="1.0" encoding="utf-8"?>
<service xmlns="http://www.thirdstage.org/example/ns/service">
	<name>MakeOrder</name>
	<input>
		<simple name="poNumber" type="string" max-length="5" min-length="10"/>
		<simple name="date" type="date"/>
	</input>
	<output>
		<simple name="totalAmount" type="decimal" min="0"/>
		<simple name="discountAmount" type="decimal" min="0"/>
		<simple name="paidAmount" type="decimal" min-exclusive="0"/>
	</output>
</service>

