<?xml version="1.0" encoding="UTF-8"?>


<data-meta>
	<id>employeeData</id>
	<elements>
		<composite name="basicInfo">
			<field name="empNo">
				<required>true</required>
				<type>string</type>
				<facets>
					<max-length>10</max-length>
					<min-length>4</min-length>
				</facets>
				<description>...</description>
			</field>
			<field name="name">
				<required>true</required>
				<type>string</type>
				<facets>
					<max-length>20</max-length>
					<min-length>1</min-length>
				</facets>
			</field>
			<field name="class">
				<required>true</required>
				<type>string</type>
				<facets>
					<enums>
						<enum><value>사원</value></enum>
						<enum><value>대리</value></enum>
						<enum><value>과장</value></enum>
						<enum><value>차장</value></enum>
						<enum><value>부장</value></enum>
						<enum><value>이사</value></enum>
						<enum>
							<value>CEO</value>
							<description>Chief Executive Officier</description>
						</enum>												
					</enums>
				</facets>
			</field>
			<field name='job'>
				<required>false</required>
				<type>string</type>
				<facets>
					<max-length>50</max-length>
				</facets>
			</field>
			<field name='job'>
				<required>false</required>
				<type>string</type>
				<facets>
					<max-length>50</max-length>
				</facets>
			</field>			
		</composite>

	</elements>
		
	
	



</data-meta>