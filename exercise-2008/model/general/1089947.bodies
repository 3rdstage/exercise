!!!8719259.java!!!	isFileTypeSupported(in fileType : int) : boolean

	int types[] = getMidiFileTypes();
	for(int i=0; i<types.length; i++) {
	    if( fileType == types[i] ) {
		return true;
	    }
	}
	return false;
