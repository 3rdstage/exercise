    class 1056667 "ControlFactory"
      abstract visibility public 
      cpp_decl ""
      java_decl "${comment}${@}${visibility}${final}${abstract}class ${name}${extends}${implements} {
${members}}
"
      php_decl ""
      python_2_2 python_decl ""
      idl_decl ""
      explicit_switch_type ""
      
      comment "This abstract class represents a factory for creating LDAPv3 controls.
LDAPv3 controls are defined in 
<A HREF=\"ftp://ftp.isi.edu/in-notes/rfc2251.txt\">RFC 2251</A>.
<p>
When a service provider receives a response control, it uses control 
factories to return the specific/appropriate control class implementation.

@author Rosanna Lee
@author Scott Seligman
@author Vincent Ryan
@version 1.12 04/07/16

@see Control
@since 1.3"
      operation 8518427 "ControlFactory"
	protected explicit_return_type ""
	nparams 0
	
	preserve_java_body_indent java_def "  ${comment}${@}${visibility}${synchronized}${name}${(}${)}${throws}${staticnl}{
  ${body}}
"
	
	
	
