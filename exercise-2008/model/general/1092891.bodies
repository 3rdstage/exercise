!!!8738971.java!!!	toString() : String
	
	StringBuffer buf = new StringBuffer();
	for (int i = 0; i < controls.length; i++) {
	    if (i != 0) {
		buf.append(", ");
		if ((i + 1) == controls.length) {
		    buf.append("and ");
