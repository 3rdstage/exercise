      operation 1917467 "prepareReplacePixels"
	public explicit_return_type "void"
	nparams 2
	  param in name "imageIndex" explicit_type "int"
	  param inout name "region" explicit_type "Rectangle"
	nexceptions 1
	  exception class_ref 179355 // IOException
	
